<h1><%= @course.name %></h1>
 <h3>Students</h3>
<table class = "table">
  <tr>
    <th>Name</th>
    <th>Grade</th>
  </tr>
  <% @course.student_name_and_grade.each do |student|%>
  <tr>
    <td><%=student.name%></td>
    <td><%=student.grade%></td>
  </tr>
  <% end %>
</table>
<% @course.students.each do |student| %>
<%= form_tag student_path(student) do |f| %>
	<%= label_tag :name %>
  <%= text_field_tag :name %>
  <%= submit_tag  "Create Student"%>
  <% end %>
  <% end %>
